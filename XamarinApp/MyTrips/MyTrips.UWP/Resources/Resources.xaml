    <BitmapImage x:Key="CurrentWhite" UriSource="ms-appx:///Assets/SplitView/current_white_32.png"/>
    <BitmapImage x:Key="CurrentBlue" UriSource="ms-appx:///Assets/SplitView/current_blue_32.png"/>

    <BitmapImage x:Key="Past" UriSource="ms-appx:///Assets/SplitView/past_32.png"/>
    <BitmapImage x:Key="PastWhite" UriSource="ms-appx:///Assets/SplitView/past_white_32.png"/>
